<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xalan="http://xml.apache.org/xslt" xmlns:j2seproject3="http://www.netbeans.org/ns/j2se-project/3" xmlns:jaxws="http://www.netbeans.org/ns/jax-ws/1">
    <!--
                ===================
                JAX-WS WSIMPORT SECTION
                ===================
            -->
    <target name="wsimport-init" depends="init">
        <mkdir dir="${build.generated.sources.dir}/jax-ws"/>
        <taskdef name="wsimport" classname="com.sun.tools.ws.ant.WsImport">
            <classpath path="${libs.jaxws21.classpath}"/>
        </taskdef>
    </target>
    <target name="wsimport-client-UploadDownloadService" depends="wsimport-init">
        <mkdir dir="${build.generated.dir}/jax-wsCache/UploadDownloadService"/>
        <property name="wsdl-UploadDownloadService" location="xml-resources/web-service-references/UploadDownloadService/wsdl/localhost_8080/community/UploadDownloadService.wsdl"/>
        <wsimport xendorsed="true" sourcedestdir="${build.generated.dir}/jax-wsCache/UploadDownloadService" destdir="${build.generated.dir}/jax-wsCache/UploadDownloadService" package="com.docdoku.client.ws.proxies.uploaddownloadservice" wsdl="${wsdl-UploadDownloadService}" catalog="catalog.xml">
            <depends file="${wsdl-UploadDownloadService}"/>
            <produces dir="${build.generated.dir}/jax-wsCache/UploadDownloadService"/>
        </wsimport>
        <copy todir="${build.generated.sources.dir}/jax-ws">
            <fileset dir="${build.generated.dir}/jax-wsCache/UploadDownloadService">
                <include name="**/*.java"/>
            </fileset>
        </copy>
    </target>
    <target name="wsimport-client-clean-UploadDownloadService" depends="-init-project">
        <delete dir="${build.generated.dir}/jax-wsCache/UploadDownloadService"/>
        <delete dir="${build.generated.sources.dir}/jax-ws/com/docdoku/client/ws/proxies/uploaddownloadservice"/>
    </target>
    <target name="wsimport-client-generate" depends="wsimport-client-UploadDownloadService"/>
</project>
