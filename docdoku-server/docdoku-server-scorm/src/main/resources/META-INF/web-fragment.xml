<web-fragment xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd" version="3.0">

    <name>ScormViewerFragment</name>

    <servlet>
        <servlet-name>ScormViewerServlet</servlet-name>
        <servlet-class>com.docdoku.server.viewers.ScormViewerServlet</servlet-class>
        <init-param>
            <param-name>DOCUMENT_VIEWER</param-name>
            <param-value>com.docdoku.server.viewers.ScormViewerImpl</param-value>
        </init-param>
    </servlet>

</web-fragment>