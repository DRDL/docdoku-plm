    <class>com.docdoku.core.entities.SerialActivityModel</class>
    <class>com.docdoku.core.entities.StateChangeSubscription</class>
    <class>com.docdoku.core.entities.Tag</class>
    <class>com.docdoku.core.entities.Task</class>
    <class>com.docdoku.core.entities.TaskModel</class>
    <class>com.docdoku.core.entities.User</class>
    <class>com.docdoku.core.entities.UserGroup</class>
