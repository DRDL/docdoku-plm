    <class>com.docdoku.core.entities.InstanceTextAttribute</class>
    <class>com.docdoku.core.entities.InstanceURLAttribute</class>
    <class>com.docdoku.core.entities.IterationChangeSubscription</class>
    <class>com.docdoku.core.entities.MasterDocument</class>
