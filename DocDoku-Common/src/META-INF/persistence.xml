    <class>com.docdoku.core.entities.Account</class>
    <class>com.docdoku.core.entities.Activity</class>
    <class>com.docdoku.core.entities.ActivityModel</class>
    <class>com.docdoku.core.entities.BinaryResource</class>
    <class>com.docdoku.core.entities.Credential</class>
    <class>com.docdoku.core.entities.Document</class>
    <class>com.docdoku.core.entities.DocumentToDocumentLink</class>
    <class>com.docdoku.core.entities.Folder</class>
    <class>com.docdoku.core.entities.InstanceAttribute</class>
    <class>com.docdoku.core.entities.InstanceAttributeTemplate</class>
    <class>com.docdoku.core.entities.InstanceBooleanAttribute</class>
    <class>com.docdoku.core.entities.InstanceDateAttribute</class>
