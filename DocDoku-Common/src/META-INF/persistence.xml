    <class>com.docdoku.core.entities.Workspace</class>
    <class>com.docdoku.core.entities.WorkspaceUserGroupMembership</class>
    <class>com.docdoku.core.entities.WorkspaceUserMembership</class>
    <properties>
      <property name="toplink.logging.level" value="FINE"/>
      <property name="toplink.ddl-generation" value="create-tables"/>
      <property name="eclipselink.ddl-generation" value="create-tables"/>
    </properties>
  </persistence-unit>
</persistence>
